{
  "name": "bddframework",
  "version": "1.0.0",
  "description": "\"Automation project for Protractor 201\"",
  "main": "index.js",
  "scripts": {
        "webdriver-update": "./node_modules/.bin/webdriver-manager update",
    "webdriver-start": "./node_modules/.bin/webdriver-manager start",
    "protractor": "./node_modules/.bin/protractor JSFiles/configuration.js",
    "web-start": "npm run webdriver-update && npm run webdriver-start",
    "RegressionSuite": "./node_modules/.bin/protractor JSFiles/configuration.js --suite regression",
    "start": " npm run protractor"
  },
 
 
  "dependencies": {
    "protractor": "^7.0.0",
    "@types/cucumber": "4.0.4",
    "@types/node": "12.12.2",
    "@types/chai": "^4.0.0",
    "chai": "^4.0.0",
    "chai-as-promised": "7.1.1",
    "cucumber": "6.0.5",
    "protractor-cucumber-framework": "6.2.1",
    "ts-node": "8.10.2",
    "typescript": "3.4.3",
    "cucumber-html-reporter": "5.2.0",
    "protractor-multiple-cucumber-html-reporter-plugin": "1.8.1"
  }
}
